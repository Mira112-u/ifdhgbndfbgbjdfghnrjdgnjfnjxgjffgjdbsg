# Telegram Bot Token
BOT_TOKEN=your_bot_token_here

# Admin User IDs (comma-separated)
ADMIN_IDS=123456789,987654321

# Co-Admin User IDs (comma-separated)
CO_ADMIN_IDS=111111111,222222222

# R&D Admin User IDs (comma-separated)
RND_ADMIN_IDS=333333333,444444444

# Log Group ID for admin action logging (optional)
LOG_GROUP_ID=-1001234567890

# Database Configuration
# SQLAlchemy database URL (supports SQLite, PostgreSQL, MySQL, etc.)
# Default: sqlite+aiosqlite:///bot_data.db (local SQLite file)
# PostgreSQL example: postgresql+asyncpg://user:password@localhost/dbname
DATABASE_URL=sqlite+aiosqlite:///bot_data.db

# Fine Monitoring Configuration
# Polling interval in seconds (default: 1800 = 30 minutes)
MONITOR_POLL_INTERVAL=1800

# Rate limit delay between requests in seconds (default: 5.0)
MONITOR_RATE_LIMIT=5.0
